{
  "name": "vue3-ts-vite-starter-template",
  "description": "Vue.js 3 TypeScript / Vite starter template with Pinia, Vue Router, Tailwind and more.",
  "version": "0.0.0-semantic-release",
  "license": "MIT",
  "repository": "https://github.com/kouts/vue3-ts-vite-starter-template",
  "packageManager": "pnpm@9.15.9",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint \"**/*.{vue,ts,js}\"",
    "lint-fix": "eslint --fix \"**/*.{vue,ts,js}\"",
    "test:unit": "vitest",
    "test:unit-coverage": "vitest run --coverage",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "pinia": "^3.0.2",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^7.0.2",
    "@fullhuman/vue-cli-plugin-purgecss": "^7.0.2",
    "@kouts/eslint-config": "link:../..",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.15.3",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitest/coverage-v8": "^3.1.2",
    "@vue/compiler-sfc": "^3.5.13",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.26.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "sass": "^1.87.0",
    "tailwindcss": "^3.4.17",
    "typescript": "5.8.3",
    "vite": "^6.3.4",
    "vitest": "^3.1.2",
    "vue-tsc": "^2.2.10"
  }
}
{
  "name": "@kouts/eslint-config",
  "type": "module",
  "version": "0.0.0-semantic-release",
  "packageManager": "pnpm@9.9.0",
  "engines": {
    "node": "^18.18.0 || ^20.9.0 || >=21.1.0"
  },
  "author": "Giannis Koutsaftakis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kouts/eslint-config/issues"
  },
  "homepage": "https://github.com/kouts/eslint-config#readme",
  "description": "Kouts's ESLint config",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./prettier": {
      "require": {
        "types": "./dist/prettier.d.cts",
        "default": "./dist/prettier.cjs"
      },
      "import": {
        "types": "./dist/prettier.d.ts",
        "default": "./dist/prettier.js"
      }
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "pkgroll --watch",
    "build": "pkgroll --clean-dist",
    "inspect": "npx @eslint/config-inspector --config eslint.config.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "pnpm run build && eslint .",
    "lint-fix": "pnpm run build && eslint --fix .",
    "prepare": "husky"
  },
  "keywords": [
    "lint",
    "eslint",
    "eslint-plugin",
    "eslint-config",
    "eslint-flat-config",
    "eslint-config-vue",
    "eslint-config-standard",
    "eslint-9-vue-config",
    "vue",
    "vue-2",
    "vue-3",
    "vue-prettier"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kouts/eslint-config.git"
  },
  "dependencies": {
    "@vitest/eslint-plugin": "^1.0.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-html": "^8.1.1",
    "eslint-plugin-import-x": "^4.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vue": "^9.27.0",
    "neostandard": "^0.11.4",
    "prettier": "^3.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/node": "^22.5.1",
    "eslint": "^9.9.1",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.9",
    "pkgroll": "^2.4.2",
    "typescript": "^5.5.4"
  },
  "peerDependencies": {
    "eslint": "^9.8.0"
  },
  "resolutions": {
    "@typescript-eslint/utils": "^8.1.0"
  }
}
{
  "name": "@kouts/eslint-config",
  "type": "module",
  "version": "0.0.0-semantic-release",
  "packageManager": "pnpm@10.25.0",
  "engines": {
    "node": "^18.18.0 || ^20.9.0 || >=21.1.0"
  },
  "author": "Giannis Koutsaftakis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kouts/eslint-config/issues"
  },
  "homepage": "https://github.com/kouts/eslint-config#readme",
  "description": "Kouts's ESLint config",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./prettier": {
      "require": {
        "types": "./dist/prettier.d.cts",
        "default": "./dist/prettier.cjs"
      },
      "import": {
        "types": "./dist/prettier.d.ts",
        "default": "./dist/prettier.js"
      }
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "pkgroll --watch",
    "build": "pkgroll --clean-dist",
    "inspect": "npx @eslint/config-inspector --config eslint.config.ts",
    "test:unit": "vitest",
    "test:unit-nowatch": "vitest run",
    "lint": "pnpm run build && eslint .",
    "lint-fix": "pnpm run build && eslint --fix .",
    "lint-fixtures": "pnpm --filter ./fixtures/vue3-ts-vite-starter-template lint",
    "typecheck": "tsc --noEmit",
    "prepare": "husky",
    "quality": "pnpm run lint && pnpm run typecheck && pnpm run test:unit-nowatch"
  },
  "keywords": [
    "lint",
    "eslint",
    "eslint-plugin",
    "eslint-config",
    "eslint-flat-config",
    "eslint-config-vue",
    "eslint-config-standard",
    "eslint-9-vue-config",
    "vue",
    "vue-2",
    "vue-3",
    "vue-prettier"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kouts/eslint-config.git"
  },
  "dependencies": {
    "@eslint/js": "^9.39.1",
    "@vitest/eslint-plugin": "^1.5.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-html": "^8.1.3",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vue": "^10.6.2",
    "neostandard": "^0.12.2",
    "prettier": "^3.7.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@types/estree": "^1.0.8",
    "@types/node": "^25.0.0",
    "eslint": "^9.39.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "pkgroll": "^2.21.4",
    "typescript": "5.9.3",
    "vitest": "^4.0.15",
    "vue-eslint-parser": "^10.2.0"
  },
  "peerDependencies": {
    "eslint": "^9.8.0"
  }
}